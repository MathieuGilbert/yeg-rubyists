<div class='memberSection' onclick="ShowLinks(this)">
  <div class='profileImage'>
    <img src="assets/placeholder.jpg" />
  </div>
  
  <div class='name'>
    <%= twitter_member.name %>
  </div>

  <div class='bottomRow'>
    <div class='memberLinks'>
      <a href=<%= "https://twitter.com/" + twitter_member.twitter %> target="_blank">
        <img src="assets/plh_twit.jpg" />
      </a>
      <a href=<%= "https://github.com/" + twitter_member.github %> target="_blank">
        <img src="assets/plh_git.jpg" />
      </a>
      <a href=<%= twitter_member.blogrss %> target="_blank">
        <img src="assets/plh_blog.jpg" />
      </a>
    </div>
    <div class='date'>
      <%= FormatUtcDate(date) %>
    </div>
  </div>
</div>

<p class='itemDetail'>
  <a href="<%= url %>" target="_blank"><%= content %></a>
</p>