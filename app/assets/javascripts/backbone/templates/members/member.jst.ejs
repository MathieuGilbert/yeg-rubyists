<div class='memberSection' onclick="ShowLinks(this)">
  <div class='profileImage'>
    <img src="assets/placeholder.jpg" />
  </div>
  
  <div class='name'>
    <%= name %>
  </div>

  <div class='memberLinks'>
    <a href=<%= "https://twitter.com/" + twitter %> target="_blank">
      <img src="assets/plh_twit.jpg" />
    </a>
    <a href=<%= "https://github.com/" + github %> target="_blank">
      <img src="assets/plh_git.jpg" />
    </a>
    <a href=<%= blogrss %> target="_blank">
      <img src="assets/plh_blog.jpg" />
    </a>
  </div>

</div>
